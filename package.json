{
  "name": "nutep_backend",
  "version": "1.0.0",
  "description": "APIs to use at APP of NÃºcleo de Terapia Nutricional Enteral e Parenteral - NUTEP",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "test": "jest --forceExit --detectOpenHandles",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:integration": "jest -c jest.integration.config.js  --passWithNoTests --no-cache --runInBand",
    "test:unit": "jest -c jest-unit-config.js  --passWithNoTests --no-cache --runInBand",
    "test:coverage": "npm test -- --coverage",
    "start": "cross-env ts-node-dev -r tsconfig-paths/register --respawn -- src/main.ts",
    "start:prod": "node dist/main"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/nutep/backend.git"
  },
  "keywords": [
    "nutep"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/nutep/backend/issues"
  },
  "homepage": "https://gitlab.com/nutep/backend#readme",
  "dependencies": {
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/mongoose": "^9.0.1",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/swagger": "^4.5.7",
    "@types/mongoose": "^5.11.97",
    "bcrypt": "^5.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.1.4",
    "mongodb-memory-server": "^6.7.6",
    "mongoose": "^6.0.12",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.0.0",
    "swagger-ui-express": "^4.1.4",
    "ts-node-dev": "^1.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.0.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "faker": "^5.5.3",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "^11.2.6",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "globalSetup": "./test/setup.js"
  }
}
